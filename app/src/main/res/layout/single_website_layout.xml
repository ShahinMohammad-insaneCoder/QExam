<?xml version="1.0" encoding="utf-8"?><RelativeLayout    xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/rlWebsiteContainer"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:background="?attr/selectableItemBackground">    <ImageView        android:id="@+id/iconWebsiteIV"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="@dimen/margin_small"        android:layout_marginTop="@dimen/margin_small"        android:tint="@color/colorPrimary"        app:srcCompat="@drawable/ic_web"        />    <TextView        android:id="@+id/websiteLinkTV"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_centerVertical="true"        android:layout_marginStart="@dimen/margin_default"        android:layout_marginTop="@dimen/margin_small"        android:layout_toStartOf="@+id/iconEditWebsiteIV"        android:layout_toEndOf="@+id/iconWebsiteIV"        android:textColor="@color/primary_text_color"        app:fontFamily="@font/roboto"        tools:text="@string/social_link"        />    <ImageView        android:id="@+id/iconEditWebsiteIV"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentEnd="true"        android:layout_margin="@dimen/margin_small"        android:background="?attr/selectableItemBackgroundBorderless"        android:clickable="true"        android:focusable="true"        app:srcCompat="@drawable/ic_pencil"/></RelativeLayout>